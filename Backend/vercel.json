{
    "installCommand":"npm install --legacy-peer-deeps",
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/static"
        },
        {
            "src":"src/**/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {"src":"/(.*)","dest":"/"}
    ]
}